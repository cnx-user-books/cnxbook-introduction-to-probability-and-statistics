 <list id="fs-idm169488192" list-type="enumerated" number-style="lower-alpha"><item>
<m:math>
 <m:mover accent="true">
  <m:mi>x</m:mi>
  <m:mo>¯</m:mo>
 </m:mover>
 
</m:math> =_______</item>
 <item>
 <m:math>
